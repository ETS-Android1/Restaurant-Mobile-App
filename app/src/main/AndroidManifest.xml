<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.restaurant_mobile_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RestaurantMobileApp">
        <activity android:name=".TableList"></activity>
        <activity android:name=".ViewPayment" />
        <activity android:name=".PaymentComplete" />
        <activity android:name=".Payment" />
        <activity android:name=".ViewOrder" />
        <activity android:name=".EditDelivery" />
        <activity android:name=".OrderComplete" />
        <activity android:name=".DeliveryOrder" />
        <activity android:name=".AboutUs" />
        <activity android:name=".CalPrice" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>